{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "gcc.exe - 활성 파일 빌드 및 디버그",
      "type": "cdbg"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Project/SocketPrac/Client",
      "program": "c:/Project/SocketPrac/Client/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}